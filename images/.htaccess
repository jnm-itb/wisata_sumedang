<IfModule mod_rewrite.c>
RewriteEngine On
RewriteRule ^crop/?$ crop.php [NC,L]
RewriteRule ^crop/([0-9-]+)/?$ crop.php?width=$1 [NC,L]
RewriteRule ^crop/([0-9-]+)/([0-9-]+)/?$ crop.php?width=$1&height=$2 [NC,L]
RewriteRule ^crop/([0-9-]+)/([0-9-]+)/(.*)/?$ crop.php?width=$1&height=$2&img=$3 [NC,L]


RewriteRule ^small/?$ small.php [NC,L]
RewriteRule ^small/([0-9-]+)/?$ small.php?width=$1 [NC,L] 
RewriteRule ^small/([0-9-]+)/(.*)/?$ small.php?width=$1&img=$2 [NC,L]

 

</IfModule>

<IfModule !mod_rewrite.c>
	ErrorDocument 404 /index.php
</IfModule>
